                                                                            //a program to find the largest value that is stored in the array
#include<stdio.h>
main()
{
int a[50], size, i, large;
printf ("\nEnter the size of the array: ");
scanf ("%d", &size);
printf ("\nEnter %d elements in to the array: ", size);
for (i=0; i<size; i++)
scanf ("%d", &a[i]);
large=a[0];
for (i=1; i<size; i++)
{
if (large<a[i])
large=a[i];
}
printf ("\nLargest: %d", large);
}
